{
    "name": "@starker-xp/baklavajs",
    "version": "2.3.3",
    "description": "Full installation of BaklavaJS including the core and all plugins",
    "author": "newcat <freddy.wagner@web.de>",
    "homepage": "https://github.com/starker-xp/baklavajs",
    "license": "MIT",
    "files": [
        "dist/*"
    ],
    "main": "dist/cjs/index.js",
    "types": "dist/index.d.ts",
    "module": "dist/esm/index.js",
    "scripts": {
        "build:esm": "tsc --declaration false --outDir dist/esm --module esnext",
        "build:cjs": "tsc --declaration false --outDir dist/cjs",
        "build:declaration": "tsc --emitDeclarationOnly --outDir dist",
        "build:browser": "esbuild src/bundle.ts --bundle --minify --sourcemap --global-name=BaklavaJS --outfile=dist/bundle.js",
        "build": "rimraf dist && yarn run build:esm && yarn run build:cjs && yarn run build:declaration && yarn build:browser"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/starker-xp/baklavajs.git"
    },
    "bugs": {
        "url": "https://github.com/starker-xp/baklavajs/issues"
    },
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@starker-xp/baklavajs-core": "^2.3.3",
        "@starker-xp/baklavajs-engine": "^2.3.3",
        "@starker-xp/baklavajs-interface-types": "^2.3.3",
        "@starker-xp/baklavajs-renderer-vue": "^2.3.3",
        "@starker-xp/baklavajs-themes": "^2.3.3"
    },
    "devDependencies": {
        "esbuild": "^0.19.11",
        "typescript": "^5.3.3"
    },
    "gitHead": "68d9c619a02506f6bd0dffc402fbbb51d17603d8"
}
